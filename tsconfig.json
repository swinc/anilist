{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "inlineSourceMap": true,
    "jsx": "react",
    "lib": ["DOM", "ES2018"],
    "module": "es2020",
    "moduleResolution": "node",
    "outDir": "./app",
    "strict": true,
    "target": "es2020"
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
